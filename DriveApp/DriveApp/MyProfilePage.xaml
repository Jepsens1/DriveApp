<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DriveApp.MyProfilePage"
             BackgroundColor="#004ba6">

    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="KmToday" Text="" FontSize="Title" TextColor="White" Padding="30,10,30,10"/>
            <Label x:Name="KmWeek" Text="" FontSize="Title" TextColor="White" Padding="30,10,30,10"/>
            <Label x:Name="WeekStats" Text="" FontSize="Body" TextColor="White" Padding="30,10,30,10"/>
            <Label x:Name="KmMonth" Text="" FontSize="Title" TextColor="White" Padding="30,10,30,10"/>
            <Label x:Name="MonthStats" Text="" FontSize="Body" TextColor="White" Padding="30,10,30,10"/>
            <Button Text="TrackingPage" BackgroundColor="White" Clicked="NavigationButton_Click" VerticalOptions="EndAndExpand"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>